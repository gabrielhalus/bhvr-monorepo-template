{
  "name": "@bunstack/shared",
  "type": "module",
  "version": "0.0.0",
  "exports": {

    "./config": {
      "types": "./dist/app.config.d.ts",
      "import": "./dist/config/app.config.js"
    },
    "./helpers": {
      "types": "./dist/helpers/index.d.ts",
      "import": "./dist/helpers/index.js"
    },
    "./lib/*": {
      "types": "./dist/lib/*.d.ts",
      "import": "./dist/lib/*.js"
    },
    "./models/*": {
      "types": "./dist/models/*.d.ts",
      "import": "./dist/models/*.js"
    },
    "./schemas/*": {
      "types": "./dist/schemas/*.d.ts",
      "import": "./dist/schemas/*.js"
    },
    "./types/*": {
      "types": "./dist/types/*.d.ts",
      "import": "./dist/types/*.js"
    }
  },
  "scripts": {
    "dev": "tsc --watch",
    "build": "tsc -b",
    "lint": "eslint .",
    "check-types": "tsc --noEmit"
  },
  "dependencies": {
    "drizzle-orm": "^0.44.2",
    "drizzle-zod": "^0.7.1",
    "nanoid": "^5.1.5"
  },
  "devDependencies": {
    "@bunstack/eslint-config": "workspace:*",
    "@bunstack/typescript-config": "workspace:*",
    "eslint": "^9.28.0"
  }
}
